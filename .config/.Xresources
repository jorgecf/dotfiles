
! xterm terminal settings

! font
xterm*faceName: xft:Inconsolata-11
! copypaste the usual way, wihout shift
xterm*translations: #override \
	Ctrl <KeyPress> C: copy-selection(CLIPBOARD) \n\
	Ctrl <KeyPress> V: insert-selection(CLIPBOARD)


! colors -- terminal.sexy, arc-theme based
! special
*.foreground:   #eeeeee
*.background:   #383c4a
*.cursorColor:  #b9b9b9

*.color0:       #101010
*.color1: 	#7c7c7c
*.color2:	#8e8e8e
*.color3:	#a0a0a0
*.color4:	#686868
*.color5:	#747474
*.color6:	#08e8de
*.color7:	#b9b9b9
